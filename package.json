  "scripts": {
    "dev": "HOST=192.168.1.8 vite",
    "func-serve": "firebase serve --only functions -p 5001",
    "ngrok": "pnpm ngrok:install && ngrok http --url=firm-swan-prompt.ngrok-free.app https://localhost:5000",
    "ngrok:install": "echo https://dashboard.ngrok.com/get-started/setup/linux?domainId=rd_2rGU62AoidL11x03u3iCojzd9ji",
    "build": "vite build",
    "serve": "vite preview",
    "deploy-hosting": "firebase deploy --only hosting",
    "deploy-functions": "firebase deploy --only functions",
    "coverage": "nyc pnpm run test:unit",
    "test:unit": "pnpm exec mocha test/unit/*.test.*js",
    "test:e2e": "cucumber-js",
    "test:func": "pnpm exec mocha --exec functions/test/*.test.*js",
    "test:reprocess": "cd functions & pnpm exec mocha --reporter spec -f reprocess",
    "admin:dev": "cd ../run-pix-admin/ && conda activate ./.conda/ && make dev",
    "express": "cd functions && export $(cat .env | grep \"^[^#;]\" |xargs) &&  node express.js ",
    "backup": "gsutil -m rsync -r -y node_modules -y .git . gs://backup-avinashmane/runpix",
    "cy:open": "cypress open",
    "vidtest": "cd functions & pnpm exec mocha --reporter spec test/video.test.js -f video ",
    "vidtest:mobile": "cd functions & npm exec mocha functions/test/video.test.js -f mobile",
    "n:build": "nuxt build",
    "n:dev": "env-cmd -f .env.development nuxt dev",
    "n:generate": "nuxt generate",
    "n:preview": "nuxt preview",
    "n:postinstall": "nuxt prepare"
  },
